{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"dom",
			"es2015"
		],
		"jsx": "react",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "./dist/ts",
		"rootDir": "./src/ts",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": [
		"src/ts/**/*"
	]
}